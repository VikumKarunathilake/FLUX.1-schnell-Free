{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "env": {
      "TOGETHER_API_KEY": "@together_api_key",
      "IMGBB_API_KEY": "@imgbb_api_key"
    }
  }
  